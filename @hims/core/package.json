  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "export": "export CORE_DIR=`pwd` && find .. -maxdepth 1 -type d -exec sh -c 'cd $0;  if [[ `dirname $0` != \".\" && `pwd` != $CORE_DIR ]]; then rsync -a -I --exclude \"node_modules\" --exclude \"cypress\" --exclude \"example\" $CORE_DIR ./node_modules/@hims/; echo \"Done exporting to $0.\"; fi;' {} \\;",
    "build:export": "yarn build && yarn export",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:ts src/**/*{.ts,.tsx}",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:ts --fix src/**/*{.ts,.tsx}",
    "lint:css": "stylelint './src/**/*{.js,.jsx,.ts,.tsx}'",
    "lint:eslint:js": "eslint -c .eslintrc-js.js --ext .js,.jsx",
    "lint:eslint:ts": "eslint -c .eslintrc-ts.js --ext .ts,.tsx",
    "lint:eslint:fix": "yarn lint:eslint:js --fix ./src/**/*{.js,.jsx} && yarn lint:eslint:ts --fix ./src/**/*{.ts,.tsx}",
    "lint:js": "yarn lint:eslint:js ./src/**/*{.js,.jsx}",
    "lint:ts": "tslint --config ./tslint.json --project ./tsconfig.json"
  },
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:eslint:js --fix ",
      "git add --force "
    ],
    "*.{md,json}": [
      "prettier --write ",
      "git add --force "
    ],
    "*.{ts,tsx}": [
      "yarn lint:eslint:ts --fix ",
      "yarn lint:ts --fix ",
      "git add --force "
    ]
  },
